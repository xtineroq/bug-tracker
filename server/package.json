{
  "name": "bug-tracker",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": ">=12.18.4"
  },
  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "nodemon --inspect=9299 ./src/server.js",
    "docs": "apidoc -i src -o docs",
    "start:other": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "seed": "node scripts/seedDB.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "apidoc": "^0.25.0",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "firebase-admin": "^9.2.0",
    "helmet": "^4.1.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "morgan": "^1.10.0",
    "yargs-parser": "^20.2.4"
  }
}
